{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"rootDir": "src",
		"noEmit": true,
		"allowImportingTsExtensions": true,
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
